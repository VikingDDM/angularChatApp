{
  "name": "angular-nest-starter",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-front-dev": "ng serve --progress=false --proxy-config proxy.conf.json",
    "prestart": "npm run build",
    "start": "node dist/server/main.js",
    "build": "ng build --prod && tsc -p ./server",
    "heroku-postbuild": "npm run build",
    "prebuild": "rimraf dist",
    "build-nest": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start-nest": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start-dev": "npm run start-nest && ng serve",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-nest-starter:server:production --bundleDependencies all"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.900.5",
    "@angular-devkit/build-optimizer": "0.11.0",
    "@angular/animations": "8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "^8.3.25",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/platform-server": "8.2.14",
    "@angular/router": "8.2.14",
    "@nestjs/cli": "^6.13.2",
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/ng-universal": "^3.0.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/platform-socket.io": "^6.11.11",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/serve-static": "^2.0.0",
    "@nestjs/testing": "^6.10.14",
    "@nestjs/websockets": "^6.11.11",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "@types/express": "^4.17.2",
    "@types/jest": "25.1.2",
    "@types/node": "^13.1.6",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "class-validator": "^0.11.0",
    "codelyzer": "^4.5.0",
    "concurrently": "^3.6.1",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~2.9.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^25.1.0",
    "jsonwebtoken": "^8.1.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mongoose": "^5.9.3",
    "ng-snotify": "^4.3.1",
    "ngx-socket-io": "^3.0.1",
    "nodemon": "^1.18.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "protractor": "^5.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "socketio-jwt": "^4.5.0",
    "supertest": "^3.0.0",
    "ts-node": "~4.1.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "^3.5.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "class-transformer": "^0.2.3",
    "ts-loader": "^5.2.0",
    "webpack-cli": "^3.1.0"
  }
}
